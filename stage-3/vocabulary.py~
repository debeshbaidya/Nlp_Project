data_file = open('new_data.txt', 'r')
vocabulary_file = open('vocabulary.txt', 'w')

words = []
line = data_file.readline().split_into_parts()
while line:
    review = line.pop(0)
    words.extend(line)
    line = data_file.readline().split_into_parts()
	
vocabulary = []
for word in words:
    if vocabulary.count(word) == 0 and words.count(word)>1:
       vocabulary.append(word)

vocabulary.sort()

for word in vocabulary:
	vocabulary_file.write(word+'\n')	
